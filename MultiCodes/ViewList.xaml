<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:v="clr-namespace:MultiCodes.Views"
                    xmlns:vm="clr-namespace:MultiCodes.ViewModels"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DataTemplate DataType="{x:Type vm:TopBar}">
        <v:TopBar />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:FileBar}">
        <v:FileBar />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CodeEditorViewModel}">
        <v:CodeEditor />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ConsoleViewModel}">
        <v:Console />
    </DataTemplate>
</ResourceDictionary>